<?xml version="1.0"?>
<launch>

    <!-- Spawns square room  -->
    <node name="spawn_room" pkg="gazebo_ros" type="spawn_model" args="-sdf -file $(find unconstrained_navigation_fetch_mapping)/launch/objects/sdf/basicRoom.sdf -model room" respawn="false" output="screen" />

    <!-- Spawns an IKEA wooden kitchen countertop -->
    <node name="spawn_table"  pkg="gazebo_ros" type="spawn_model" args="-urdf -file $(find unconstrained_navigation_fetch_mapping)/launch/objects/kitchen_counter.urdf -model table -x 2.3 -y 2.0 -z 0.05" respawn="false" output="screen" />

    <!-- Spawns a simple jar with a red lid -->
        <node name="spawn_bb" pkg="gazebo_ros" type="spawn_model" args="-urdf -file $(find unconstrained_navigation_fetch_mapping)/launch/objects/bean_box.urdf -model box   -x 2.2 -y 1.6 -z 0.99 " respawn="false" output="screen" />


</launch>


